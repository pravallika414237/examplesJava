package basicpackage;

public class ThreeDArray {

	public static void main(String[] args) {
		// int s1[][]= {{41,22,35},{54,93,89}};
		 //int s2[][]= {{89,58,63},{58,25,20}};
		int count=1;
		int marks[][][]=
			{
				{
					{41,22,35,89},
					{54,93,89}
				},
				{
					{89,58,54,45,69,63},
					{58,25,20}
				}
		    };

		for(int s[][]:marks) {
			System.out.println("student : "+count);
			for(int []i:s) {
				
				for(int j:i) {
					 System.out.print( j+"  ");
	               }
		             System.out.println(" ");
				}
			count++;
				
			}
		}
	}


